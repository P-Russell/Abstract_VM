push int32(42)

push int32(33) ; Another comment

add

push float(44.55)

mul

push doble(42.42)

push int32(42) ble

dump

pop

assert double(42.42) ; Asserts that a value at the top of the stack 
;is equal to the value passed - if false programs execution stops with error

exit
